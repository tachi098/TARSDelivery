@model TARSDeliveryWebApp.Models.PriceList

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>PriceList</h4>
<hr />
<div class="container-fluid mt-3">
    <div class="col-lg-12">
        <div class="col-lg-12">
            <form asp-action="Create" asp-controller="PriceList" asp-area="Admin">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PriceDistance" class="control-label"></label>
                    <input asp-for="PriceDistance" class="form-control" />
                    <span asp-validation-for="PriceDistance" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PriceWeight" class="control-label"></label>
                    <input asp-for="PriceWeight" class="form-control" />
                    <span asp-validation-for="PriceWeight" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>